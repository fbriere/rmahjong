# mfgen configuration -- don't use quotes
CC=gcc
SRC_EXTENSION=c
INCLUDES=
LIB=
LIBDIR=
CFLAGS=-Wall -O3 -march=native -fomit-frame-pointer
BACKUP_MAKEFILE=no

# pravidla
.SUFFIXES: .c
.c.o:
	gcc $(CFLAGS) $(INCLUDES) -c $< -o $@
	

# main rule
bot:  eval.o gamecontext.o io.o main.o score.o tiles.o tools.o
	gcc -o $@ $(INCLUDES) $(LIB) $(LIBDIR) $(ARG)  eval.o gamecontext.o io.o main.o score.o tiles.o tools.o


clean:
	@echo
	@echo Running "make clean"...
	@rm *.o core "bot" 2> /dev/null || true


# c file rules
eval.o: eval.c tools.h eval.h tiles.h gamecontext.h score.h io.h
gamecontext.o: gamecontext.c gamecontext.h tiles.h
io.o: io.c io.h tiles.h gamecontext.h
main.o: main.c tools.h eval.h tiles.h gamecontext.h score.h io.h
score.o: score.c score.h tiles.h
tiles.o: tiles.c tiles.h
tools.o: tools.c tools.h

# manual makefile update
dep: clean
	@echo
	@echo Generating makefile...
	@"/home/spirali/bin/mfgen-2.0"
	@make -f "makefile" clean

# eof

